<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="278.0" maxWidth="543.0" prefHeight="249.0" prefWidth="543.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="extension.GBuildTools">
   <children>
      <AnchorPane layoutY="216.0" prefHeight="37.0" prefWidth="543.0" style="-fx-background-color: #eee;">
         <children>
            <CheckBox fx:id="enable_gbuildtools" layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#enable_tgl" text="Enable G-BuildTools" />
            <Label fx:id="room_found_lbl" layoutX="159.0" layoutY="11.0" text="No room found" textFill="RED" />
            <Label fx:id="furnidata_lbl" layoutX="276.0" layoutY="11.0" text="Furnidata not loaded" textFill="RED" />
            <Label fx:id="stack_tile_lbl" layoutX="416.0" layoutY="11.0" text="No stack tile found" textFill="RED" />
         </children>
      </AnchorPane>
      <TabPane layoutX="-1.0" prefHeight="211.0" prefWidth="544.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="General">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="293.0" prefWidth="577.0">
                     <children>
                        <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="154.0" prefWidth="146.0" style="-fx-border-color: grey; -fx-border-radius: 3px;">
                           <children>
                              <Label layoutX="31.0" layoutY="-8.0" style="-fx-background-color: white;" text="Quickdrop furni" />
                              <CheckBox fx:id="qd_floor_cbx" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" selected="true" text="Floor furniture" textFill="#000000d7" />
                              <CheckBox fx:id="qd_wall_cbx" layoutX="14.0" layoutY="44.0" mnemonicParsing="false" selected="true" text="Wall furniture" textFill="#000000d6" />
                              <CheckBox fx:id="override_rotation_cbx" layoutX="14.0" layoutY="87.0" mnemonicParsing="false" onAction="#toggleOverrideRotation" text="Override rotation:" textFill="#000000d6" />

                               <Spinner fx:id="override_rotation_spinner" disable="true" editable="true" layoutX="14.0" layoutY="115.0" prefHeight="25.0" prefWidth="115.0">
                                   <valueFactory>
                                       <SpinnerValueFactory.IntegerSpinnerValueFactory amountToStepBy="1" initialValue="0" max="7" min="0" />
                                   </valueFactory>
                               </Spinner>
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="170.0" layoutY="14.0" prefHeight="154.0" prefWidth="146.0" style="-fx-border-color: grey; -fx-border-radius: 3px;">
                           <children>
                              <Label layoutX="29.0" layoutY="-8.0" style="-fx-background-color: white;" text="Wired duplicator" />
                              <RadioButton fx:id="rd_wired_none" layoutX="14.0" layoutY="48.0" mnemonicParsing="false" selected="true" text="None" textFill="#000000d6">
                                 <toggleGroup>
                                    <ToggleGroup fx:id="wired_tgl" />
                                 </toggleGroup>
                              </RadioButton>
                              <RadioButton fx:id="rd_wired_cond" disable="true" layoutX="14.0" layoutY="73.0" mnemonicParsing="false" text="Last condition" textFill="#000000d6" toggleGroup="$wired_tgl" />
                              <RadioButton fx:id="rd_wired_effect" disable="true" layoutX="14.0" layoutY="98.0" mnemonicParsing="false" text="Last effect" textFill="#000000d6" toggleGroup="$wired_tgl" />
                              <RadioButton fx:id="rd_wired_trig" disable="true" layoutX="14.0" layoutY="123.0" mnemonicParsing="false" text="Last trigger" textFill="#000000d6" toggleGroup="$wired_tgl" />
                              <Label layoutX="14.0" layoutY="21.0" text="Duplicate from:" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="323.0" layoutY="14.0" prefHeight="70.0" prefWidth="204.0" style="-fx-border-color: grey; -fx-border-radius: 3px;">
                           <children>
                              <Label layoutX="65.0" layoutY="-8.0" style="-fx-background-color: white;" text="Stacktile tools" />
                              <CheckBox fx:id="st_allstacktile_cbx" disable="true" layoutX="12.0" layoutY="9.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="179.0" text="Apply stacktile updates to all stacktiles in room" textFill="#000000d6" wrapText="true" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="323.0" layoutY="98.0" prefHeight="70.0" prefWidth="204.0" style="-fx-border-color: grey; -fx-border-radius: 3px;">
                           <children>
                              <Label layoutX="52.0" layoutY="-8.0" style="-fx-background-color: white;" text="Invisible furni tools" />
                              <CheckBox fx:id="ift_pizza_cbx" disable="true" layoutX="12.0" layoutY="12.0" mnemonicParsing="false" onAction="#tbones_tgl" prefHeight="53.0" prefWidth="179.0" text="Display T-Bones as pizza boxes" textFill="#000000d6" wrapText="true" />
                           </children>
                        </AnchorPane>
                     </children></AnchorPane>
            </content>
          </Tab>
           <Tab text="Furni mover">
              <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <AnchorPane layoutX="14.0" layoutY="13.0" prefHeight="129.0" prefWidth="166.0" style="-fx-background-color: #f2f2f2; -fx-background-radius: 5px; -fx-border-color: #ccc; -fx-border-radius: 5px;">
                           <children>
                              <RadioButton fx:id="rd_fm_mode_tile" layoutX="9.0" layoutY="32.0" mnemonicParsing="false" onAction="#onChangeMovementMode" selected="true" text="Tile" textFill="#000000d6">
                                 <toggleGroup>
                                    <ToggleGroup fx:id="mv_mode_tgl" />
                                 </toggleGroup>
                              </RadioButton>
                              <Label layoutX="9.0" layoutY="8.0" text="Movement mode:" />
                              <RadioButton fx:id="rd_fm_mode_rect" layoutX="9.0" layoutY="74.0" mnemonicParsing="false" onAction="#onChangeMovementMode" text="Rectangle" textFill="#000000d6" toggleGroup="$mv_mode_tgl" />
                              <CheckBox fx:id="fm_cbx_inversedir" disable="true" layoutX="9.0" layoutY="101.0" mnemonicParsing="false" text="Inverse direction" textFill="#000000d6" />
                              <RadioButton fx:id="rd_fm_mode_auto" layoutX="9.0" layoutY="53.0" mnemonicParsing="false" onAction="#onChangeMovementMode" text="Auto" textFill="#000000d6" toggleGroup="$mv_mode_tgl" />
                           </children>
                        </AnchorPane>
                        <AnchorPane layoutX="196.0" layoutY="13.0" prefHeight="129.0" prefWidth="218.0" style="-fx-background-color: #f2f2f2; -fx-background-radius: 5px; -fx-border-color: #ccc; -fx-border-radius: 5px;">
                           <children>
                              <CheckBox fx:id="fm_cbx_usestacktile" disable="true" layoutX="8.0" layoutY="9.0" mnemonicParsing="false" onAction="#updateUI" selected="true" text="Use stacktile (2x2)" textFill="#000000d6" />
                              <Spinner fx:id="height_offset_spinner" disable="true" editable="true" layoutX="109.0" layoutY="64.0" prefHeight="25.0" prefWidth="83.0">
                                 <valueFactory>
                                    <SpinnerValueFactory.DoubleSpinnerValueFactory amountToStepBy="0.1" initialValue="0.0" max="60.0" min="-50.0" />
                                 </valueFactory>
                              </Spinner>
                              <RadioButton fx:id="rd_fm_stack_matchheight" disable="true" layoutX="8.0" layoutY="37.0" mnemonicParsing="false" onAction="#updateUI" selected="true" text="Match height">
                                 <toggleGroup>
                                    <ToggleGroup fx:id="mv_stck_tgl" />
                                 </toggleGroup></RadioButton>
                              <RadioButton fx:id="rd_fm_stack_offset" disable="true" layoutX="8.0" layoutY="68.0" mnemonicParsing="false" onAction="#updateUI" text="Offset:" toggleGroup="$mv_stck_tgl" />
                              <Spinner fx:id="flatten_height_spinner" disable="true" editable="true" layoutX="109.0" layoutY="95.0" prefHeight="25.0" prefWidth="83.0">
                                 <valueFactory>
                                    <SpinnerValueFactory.DoubleSpinnerValueFactory amountToStepBy="0.1" initialValue="0.0" max="60.0" min="-50.0" />
                                 </valueFactory>
                              </Spinner>
                              <RadioButton fx:id="rd_fm_stack_flatten" disable="true" layoutX="8.0" layoutY="99.0" mnemonicParsing="false" onAction="#updateUI" text="Flatten at:" toggleGroup="$mv_stck_tgl" />
                           </children>
                        </AnchorPane>
                        <CheckBox fx:id="fm_cbx_rotatefurni" layoutX="14.0" layoutY="148.0" mnemonicParsing="false" text="Rotate furniture" textFill="#000000d6" />
                        <CheckBox fx:id="fm_cbx_wiredsafety" layoutX="132.0" layoutY="148.0" mnemonicParsing="false" selected="true" text="Wired safety" />
                        <CheckBox fx:id="fm_cbx_visualhelp" layoutX="236.0" layoutY="148.0" mnemonicParsing="false" selected="true" text="Visual help" />
                        <AnchorPane layoutX="434.0" layoutY="13.0" prefHeight="154.0" prefWidth="94.0" style="-fx-border-width: 1px; -fx-border-color: #ccc;">
                           <children>
                              <Label layoutX="9.0" layoutY="6.0" text="Commands:" />
                              <Label layoutX="12.0" layoutY="27.0" text=":move / :m" textFill="#000000b2" />
                              <Label layoutX="12.0" layoutY="46.0" text=":abort / :a" textFill="#000000b2" />
                              <Label layoutX="12.0" layoutY="65.0" text=":undo / :u" textFill="#000000b2" />
                           </children>
                        </AnchorPane>
                     </children></AnchorPane>
              </content>
           </Tab>
           <Tab text="Poster mover">
              <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
           </Tab>
           <Tab text="Settings">
              <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Hyperlink layoutX="3.0" layoutY="3.0" text="Read the github README usage instructions" />
                        <CheckBox fx:id="always_on_top_cbx" layoutX="7.0" layoutY="24.0" mnemonicParsing="false" onAction="#toggleAlwaysOnTop" prefHeight="31.0" prefWidth="109.0" text="Always on top" />
                     </children></AnchorPane>
              </content>
           </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
